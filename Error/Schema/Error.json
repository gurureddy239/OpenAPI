{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://netapp.com/json/schemas/error",
    "type": "object",
    "properties": {
        "error": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                },
                "message": {
                    "type": "string"
                },
                "error_subcode": {
                    "type": "integer"
                },
                "tracking_ids": {
                    "type": "array",
                    "items": [
                        {
                            "type": "object",
                            "properties": {
                                "key": {
                                    "type": "string"
                                },
                                "value": {
                                    "type": "string"
                                }
                            }
                        }
                    ]
                }
            },
            "required": [
                "message",
                "type",
                "code"
            ]
        }
    },
    "required": [
        "error"
    ]
}